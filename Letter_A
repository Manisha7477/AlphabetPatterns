package com.kodnest.alphabet;

public class ASolution {
	public static void main(String[] args) {
		for(int i=1;i<=10;i++) {
			for(int j=1 ;j<=10;j++) {
				if(i<=10 && j==1 || i==1 && j<=10 || 1<=10 && j==10 || i==5 && j<=10) {
				System.out.print("$S");
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
